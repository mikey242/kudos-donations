{
  "name": "iseard-media/kudos-donations",
  "description": "",
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "typisttech/imposter-plugin": false,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "php": "^8.0",
    "symfony/polyfill-mbstring": "1.20.*",
    "symfony/polyfill-ctype": "1.20.*",
    "mollie/mollie-api-php": "^2.32.0",
    "monolog/monolog": "^2.8",
    "twig/twig": "^3.0",
    "ext-json": "*",
    "psr/log": "1.1.*",
    "php-di/php-di": "^7.0",
    "php-di/invoker": "2.0.0",
    "woocommerce/action-scheduler": "^3.5",
    "symfony/dotenv": "^5.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.7.2",
    "wp-coding-standards/wpcs": "2.3.0",
    "phpcompatibility/phpcompatibility-wp": "*",
    "phpcompatibility/php-compatibility": "^9.3",
    "sirbrillig/phpcs-variable-analysis": "v2.11.16",
    "slevomat/coding-standard": "^8.6",
    "filp/whoops": "^2.13",
    "phpunit/phpunit": "8.*",
    "phpstan/phpstan": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "IseardMedia\\Kudos\\": "inc/"
    }
  }
}
