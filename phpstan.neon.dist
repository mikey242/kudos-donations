services:
includes:
	- common/vendor/phpstan/phpstan/conf/bleedingEdge.neon

parameters:
  level: 5
  inferPrivatePropertyTypeFromConstructor: true
  checkGenericClassInNonGenericObjectType: false
  checkMissingIterableValueType: false
  ignoreErrors:
  paths:
    - includes/
  scanDirectories:
  bootstrapFiles:
    - kudos-donations.php