{
	"name": "kudos-donations",
	"version": "4.0.0-alpha-17",
	"private": true,
	"description": " Add a donation button to any page on your website. Easy & fast setup. Works with Mollie payments.",
	"author": "Michael Iseard",
	"keywords": [
		"donation",
		"mollie",
		"wordpress"
	],
	"homepage": "https://kudosdonations.com",
	"repository": "https://gitlab.iseard.media/michael/kudos-donations",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"bugs": {
		"url": "https://gitlab.iseard.media/michael/kudos-donations/issues",
		"email": "michael@iseard.media"
	},
	"dependencies": {
		"@headlessui/react": "2.1.6",
		"@heroicons/react": "^2.1.5",
		"@wordpress/core-data": "^7.6.0",
		"@wordpress/data": "^10.7.0",
		"@wordpress/dataviews": "^4.3.0",
		"@wordpress/dom-ready": "^4.7.0",
		"@wordpress/notices": "^5.7.0",
		"clsx": "^2.1.1",
		"focus-trap-react": "^10.2.3",
		"react-hook-form": "^7.53.0",
		"react-mailchimp-subscribe": "^2.1.3",
		"react-router-dom": "^6.26.2",
		"react-select-country-list": "^2.2.3",
		"react-shadow-root": "^6.2.0"
	},
	"devDependencies": {
		"@automattic/i18n-loader-webpack-plugin": "^2.0.58",
		"@babel/preset-react": "^7.24.7",
		"@tailwindcss/forms": "^0.5.9",
		"@wordpress/api-fetch": "^7.7.0",
		"@wordpress/babel-preset-default": "^8.7.0",
		"@wordpress/block-editor": "^14.2.0",
		"@wordpress/blocks": "^13.7.0",
		"@wordpress/browserslist-config": "^6.7.0",
		"@wordpress/components": "^28.7.0",
		"@wordpress/compose": "^7.7.0",
		"@wordpress/date": "^5.7.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^6.7.0",
		"@wordpress/element": "^6.7.0",
		"@wordpress/env": "^10.7.0",
		"@wordpress/eslint-plugin": "^21.0.0",
		"@wordpress/i18n": "^5.7.0",
		"@wordpress/keycodes": "^4.7.0",
		"@wordpress/scripts": "^29.0.0",
		"@wordpress/stylelint-config": "^22.7.0",
		"autoprefixer": "^10.4.20",
		"lodash": "^4.17.21",
		"postcss": "^8.4.45",
		"postcss-import": "^16.1.0",
		"prettier": "^3.3.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"stylelint": "^14.16.1",
		"stylelint-config-tailwindcss": "^0.0.7",
		"tailwindcss": "^3.4.10",
		"terser-webpack-plugin": "^5.3.10",
		"webpack": "^5.94.0",
		"webpack-cli": "^5.1.4"
	},
	"scripts": {
		"build": "npm run build:admin && npm run build:front",
		"build:admin": "wp-scripts build --webpack-src-dir=src/admin src/admin/kudos-admin-settings src/admin/kudos-admin-campaigns --output-path=build/admin",
		"build:front": "wp-scripts build --webpack-src-dir=src/front --output-path=build/front",
		"check-engines": "wp-scripts check-engines",
		"start:admin": "wp-scripts start --hot --webpack-src-dir=src/admin src/admin/kudos-admin-settings src/admin/kudos-admin-campaigns --output-path=build/admin",
		"start:front": "wp-scripts start --hot --webpack-src-dir=src/front --output-path=build/front",
		"wp-env": "wp-env",
		"make-pot": "wp i18n make-pot . --exclude='src'",
		"make-json": "wp i18n make-json languages/ --no-purge",
		"follow-log": "source ./.env && tail -f -n 200 ./wordpress/storage/logs/$APP_ENV.log",
		"serveo": "ssh -R 80:localhost:8888 serveo.net",
		"test:unit": "wp-env run tests-cli --env-cwd=wp-content/plugins/kudos-donations ./vendor/bin/phpunit",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style 'src/**/*.css'",
		"lint:js": "wp-scripts lint-js ./src",
		"packages-update": "wp-scripts packages-update"
	},
	"wp-env": {
		"plugin-dir": "kudos-donations",
		"plugin-name": "Kudos Donations",
		"welcome-build-command": "npm run production"
	}
}
