{
  "name": "kudos-donations",
  "version": "4.0.0-beta-17",
  "private": true,
  "description": " Add a donation button to any page on your website. Easy & fast setup. Works with Mollie payments.",
  "author": "Michael Iseard",
  "keywords": [
    "donation",
    "mollie",
    "wordpress"
  ],
  "homepage": "https://kudosdonations.com",
  "repository": "https://gitlab.iseard.media/michael/kudos-donations",
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "bugs": {
    "url": "https://gitlab.iseard.media/michael/kudos-donations/issues",
    "email": "michael@iseard.media"
  },
  "dependencies": {
    "@automattic/babel-plugin-preserve-i18n": "^1.0.0",
    "@headlessui/react": "2.2.0",
    "@heroicons/react": "^2.1.5",
    "clsx": "^2.1.1",
    "focus-trap-react": "^11.0.0",
    "react-hook-form": "^7.53.2",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-router-dom": "^7.0.0",
    "react-select-country-list": "^2.2.3",
    "react-shadow": "^20.5.0"
  },
  "devDependencies": {
    "@automattic/i18n-loader-webpack-plugin": "^2.0.63",
    "@babel/preset-react": "^7.25.9",
    "@tailwindcss/forms": "^0.5.9",
    "@wordpress/api-fetch": "^7.18.0",
    "@wordpress/babel-preset-default": "^8.18.0",
    "@wordpress/block-editor": "^14.13.0",
    "@wordpress/blocks": "^14.7.0",
    "@wordpress/browserslist-config": "^6.18.0",
    "@wordpress/components": "^29.4.0",
    "@wordpress/compose": "^7.18.0",
    "@wordpress/core-data": "^7.18.0",
    "@wordpress/data": "^10.18.0",
    "@wordpress/dataviews": "^4.14.0",
    "@wordpress/date": "^5.18.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^6.18.0",
    "@wordpress/dom-ready": "^4.18.0",
    "@wordpress/element": "^6.18.0",
    "@wordpress/env": "^10.18.0",
    "@wordpress/eslint-plugin": "^22.4.0",
    "@wordpress/hooks": "^4.18.0",
    "@wordpress/i18n": "^5.18.0",
    "@wordpress/keycodes": "^4.18.0",
    "@wordpress/notices": "^5.18.0",
    "@wordpress/scripts": "^30.11.0",
    "@wordpress/stylelint-config": "^23.10.0",
    "autoprefixer": "^10.4.20",
    "lodash": "^4.17.21",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stylelint": "^16.10.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^3.4.15",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.96.1",
    "webpack-cli": "^6.0.0",
    "webpack-merge": "^6.0.1"
  },
  "scripts": {
    "build": "npm run build:admin && npm run build:front",
    "build:admin": "wp-scripts build --config src/admin/webpack.config.admin.js --webpack-src-dir=src/admin --output-path=build/admin",
    "build:front": "wp-scripts build --config src/block/webpack.config.front.js --output-path=build/front",
    "check-engines": "wp-scripts check-engines",
    "start:admin": "wp-scripts start --hot --config src/admin/webpack.config.admin.js --webpack-src-dir=src/admin --output-path=build/admin",
    "start:front": "wp-scripts start --hot --config src/block/webpack.config.front.js --output-path=build/front",
    "wp-env": "wp-env",
    "make-pot": "wp i18n make-pot . --exclude='src, wordpress, config, assets, tests'",
    "make-json": "wp i18n make-json languages/ --no-purge",
    "follow-log": "source ./.env && tail -f -n 200 ./wordpress/storage/logs/$APP_ENV.log",
    "serveo": "ssh -R 80:localhost:8888 serveo.net",
    "test:unit": "wp-env run tests-cli --env-cwd=wp-content/plugins/kudos-donations ./vendor/bin/phpunit",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style 'src/**/*.css'",
    "lint:js": "wp-scripts lint-js ./src",
    "packages-update": "wp-scripts packages-update",
    "make-zip": "rm -f kudos-donations.zip && php wp-cli.phar dist-archive . ./kudos-donations.zip --allow-root"
  },
  "wp-env": {
    "plugin-dir": "kudos-donations",
    "plugin-name": "Kudos Donations",
    "welcome-build-command": "npm run production"
  }
}
