{
  "name": "kudos-donations",
  "version": "3.2.8",
  "description": " Add a donation button to any page on your website. Easy & fast setup. Works with Mollie payments.",
  "homepage": "https://gitlab.iseard.media/michael/kudos-donations",
  "scripts": {
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production",
    "make-pot": "wp i18n make-pot . --exclude='docker, src, node_modules, vendor'",
    "make-json": "wp i18n make-json languages/ --no-purge",
    "export": "composer run release",
    "wp-env": "wp-env"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.25.7",
    "@wordpress/api-fetch": "^7.9.0",
    "@wordpress/babel-preset-default": "^8.9.0",
    "@wordpress/block-editor": "^14.4.0",
    "@wordpress/blocks": "^13.9.0",
    "@wordpress/browserslist-config": "^6.9.0",
    "@wordpress/components": "^28.9.0",
    "@wordpress/compose": "^7.9.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^6.9.0",
    "@wordpress/element": "^6.9.0",
    "@wordpress/env": "^10.9.0",
    "@wordpress/hooks": "^4.9.0",
    "@wordpress/i18n": "^5.9.0",
    "@wordpress/keycodes": "^4.9.0",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "laravel-mix": "^6.0.49",
    "mini-svg-data-uri": "^1.4.4",
    "postcss-loader": "^8.1.1",
    "postcss-prefixwrap": "^1.52.0",
    "tailwindcss": "^3.4.13",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "classnames": "^2.5.1",
    "jquery-validation": "^1.21.0",
    "lodash": "^4.17.21",
    "party-js": "^2.2.0"
  }
}
