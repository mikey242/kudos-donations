{
  "name": "kudos-donations",
  "version": "4.0.0-beta-6",
  "private": true,
  "description": " Add a donation button to any page on your website. Easy & fast setup. Works with Mollie payments.",
  "author": "Michael Iseard",
  "keywords": ["donation", "mollie", "wordpress"],
  "homepage": "https://kudosdonations.com",
  "repository": "https://gitlab.iseard.media/michael/kudos-donations",
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "bugs": {
    "url": "https://gitlab.iseard.media/michael/kudos-donations/issues",
    "email": "michael@iseard.media"
  },
  "dependencies": {
    "@headlessui/react": "1.7.18",
    "@heroicons/react": "^2.1.1",
    "classnames": "^2.5.1",
    "focus-trap-react": "^10.1.4",
    "react-colorful": "^5.6.1",
    "react-hook-form": "^7.50.1",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-router-dom": "^6.22.0",
    "react-select-country-list": "^2.2.3",
    "react-shadow-root": "^6.1.1",
    "use-query-params": "^2.2.1"
  },
  "devDependencies": {
    "@automattic/babel-plugin-preserve-i18n": "^1.0.0",
    "@automattic/i18n-check-webpack-plugin": "^1.1.6",
    "@automattic/i18n-loader-webpack-plugin": "^2.0.45",
    "@babel/preset-react": "^7.23.3",
    "@tailwindcss/forms": "^0.5.7",
    "@wordpress/api-fetch": "^6.48.0",
    "@wordpress/babel-plugin-makepot": "^5.35.0",
    "@wordpress/babel-preset-default": "^7.35.0",
    "@wordpress/block-editor": "^12.19.1",
    "@wordpress/blocks": "^12.28.1",
    "@wordpress/browserslist-config": "^5.34.0",
    "@wordpress/components": "^26.0.1",
    "@wordpress/compose": "^6.28.0",
    "@wordpress/date": "^4.51.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^5.2.0",
    "@wordpress/element": "^5.28.0",
    "@wordpress/env": "^9.3.0",
    "@wordpress/eslint-plugin": "^17.8.0",
    "@wordpress/i18n": "^4.51.0",
    "@wordpress/keycodes": "^3.51.0",
    "@wordpress/scripts": "^27.2.0",
    "autoprefixer": "^10.4.17",
    "lodash": "^4.17.21",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "webpack": "^5.90.2",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "npm run build:admin && npm run build:front",
    "build:admin": "wp-scripts build --webpack-src-dir=src/admin src/admin/kudos-admin-settings.jsx src/admin/kudos-admin-campaigns.jsx --output-path=build/admin",
    "build:front": "wp-scripts build --webpack-src-dir=src/front --output-path=build/front",
    "check-engines": "wp-scripts check-engines",
    "start:admin": "wp-scripts start --hot --webpack-src-dir=src/admin src/admin/kudos-admin-settings.jsx src/admin/kudos-admin-campaigns.jsx --output-path=build/admin",
    "start:front": "wp-scripts start --hot --webpack-src-dir=src/front --output-path=build/front",
    "wp-env": "wp-env",
    "make-pot": "wp i18n make-pot . --exclude='src'",
    "make-json": "wp i18n make-json languages/ --no-purge",
    "follow-log": "source ./.env && tail -f -n 200 ./uploads/logs/$APP_ENV.log",
    "serveo": "ssh -R 80:localhost:8888 serveo.net",
    "test:unit": "wp-env run tests-cli --env-cwd=wp-content/plugins/kudos-donations ./vendor/bin/phpunit"
  },
  "wp-env": {
    "plugin-dir": "kudos-donations",
    "plugin-name": "Kudos Donations",
    "welcome-build-command": "npm run production"
  }
}
