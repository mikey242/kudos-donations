{
  "name": "kudos-donations",
  "version": "2.1.1",
  "description": " Add a donation button to any page on your website. Easy & fast setup. Works with Mollie payments.",
  "homepage": "https://gitlab.iseard.media/michael/kudos-donations",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "build:production": "cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && composer install --no-dev --optimize-autoloader",
    "make-pot": "wp i18n make-pot . --exclude='libraries, docker'",
    "make-json": "wp i18n make-json languages/ --no-purge",
    "export": "./export.sh"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@wordpress/babel-preset-default": "^4.19.0",
    "@wordpress/browserslist-config": "^2.7.0",
    "@wordpress/eslint-plugin": "^7.3.0",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.15.0",
    "eslint-webpack-plugin": "^2.4.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.3",
    "mini-svg-data-uri": "^1.2.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.0",
    "webpack": "^5.0.0",
    "webpack-assets-manifest": "^5.0.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "axios": "^0.21.0",
    "install": "^0.13.0",
    "jquery": "^3.5.1",
    "jquery-validation": "1.19.2",
    "lodash": "^4.17.20",
    "micromodal": "^0.4.6",
    "parsley": "^0.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^2.0.2"
  }
}
