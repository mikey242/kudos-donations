{
  "name": "kudos-donations",
  "version": "2.0.5",
  "description": " Add a donation button to any page on your website. Easy & fast setup. Works with Mollie payments.",
  "homepage": "https://gitlab.iseard.media/michael/kudos-donations",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack --config webpack.dev.js",
    "build": "cross-env NODE_ENV=prod webpack --config webpack.prod.js",
    "build:production": "cross-env NODE_ENV=prod webpack --env.NODE_ENV=local --env.production --progress --config webpack.prod.js && composer install --no-dev --optimize-autoloader",
    "make-pot": "wp i18n make-pot . --exclude='libraries, docker'",
    "make-json": "wp i18n make-json languages/ --no-purge",
    "export": "./export.sh"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.10.1",
    "@wordpress/babel-preset-default": "^4.17.0",
    "@wordpress/browserslist-config": "^2.6.0",
    "@wordpress/eslint-plugin": "^7.1.0",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.5.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mini-svg-data-uri": "^1.1.3",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.7",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^3.0.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "axios": "^0.19.2",
    "install": "^0.13.0",
    "jquery": "^3.5.1",
    "jquery-validation": "^1.19.1",
    "lodash": "^4.17.15",
    "micromodal": "^0.4.6",
    "parsley": "^0.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^2.0.1"
  }
}
