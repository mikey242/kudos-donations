{
  "name": "kudos-donations",
  "version": "4.0.0-beta-6",
  "private": true,
  "description": " Add a donation button to any page on your website. Easy & fast setup. Works with Mollie payments.",
  "homepage": "https://gitlab.iseard.media/michael/kudos-donations",
  "scripts": {
    "build": "npm run build:admin && npm run build:front",
    "build:admin": "wp-scripts build --webpack-src-dir=src/admin src/admin/kudos-admin-settings.jsx src/admin/kudos-admin-campaigns.jsx --output-path=build/admin",
    "build:front": "wp-scripts build --webpack-src-dir=src/front --output-path=build/front",
    "check-engines": "wp-scripts check-engines",
    "start:admin": "wp-scripts start --hot --webpack-src-dir=src/admin src/admin/kudos-admin-settings.jsx src/admin/kudos-admin-campaigns.jsx --output-path=build/admin",
    "start:front": "wp-scripts start --hot --webpack-src-dir=src/front --output-path=build/front",
    "wp-env": "wp-env",
    "make-pot": "wp i18n make-pot . --exclude='src'",
    "make-json": "wp i18n make-json languages/ --no-purge"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "devDependencies": {
    "@automattic/babel-plugin-preserve-i18n": "^1.0.0",
    "@automattic/i18n-check-webpack-plugin": "^1.0.36",
    "@automattic/i18n-loader-webpack-plugin": "^2.0.37",
    "@babel/preset-react": "^7.22.15",
    "@tailwindcss/forms": "^0.5.6",
    "@wordpress/babel-plugin-makepot": "^5.26.0",
    "@wordpress/babel-preset-default": "^7.26.0",
    "@wordpress/block-editor": "^12.10.0",
    "@wordpress/blocks": "^12.19.0",
    "@wordpress/browserslist-config": "^5.25.0",
    "@wordpress/components": "^25.8.0",
    "@wordpress/compose": "^6.19.0",
    "@wordpress/date": "^4.42.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^4.25.0",
    "@wordpress/env": "^8.8.0",
    "@wordpress/eslint-plugin": "^16.0.0",
    "@wordpress/keycodes": "^3.42.0",
    "@wordpress/scripts": "^26.13.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.24",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.2",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@headlessui/react": "1.7.15",
    "@heroicons/react": "^2.0.18",
    "@wordpress/api-fetch": "^6.32.0",
    "@wordpress/element": "^5.12.0",
    "@wordpress/i18n": "^4.35.0",
    "classnames": "^2.3.1",
    "focus-trap-react": "^10.1.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-select-country-list": "^2.2.3",
    "react-shadow-root": "^6.1.1"
  },
  "wp-env": {
    "plugin-dir": "kudos-donations",
    "plugin-name": "Kudos Donations",
    "welcome-build-command": "npm run production"
  }
}
