{
	"name": "kudos-donations",
	"version": "4.2.0",
	"private": true,
	"description": " Add a donation button to any page on your website. Easy & fast setup. Works with Mollie payments.",
	"author": "Michael Iseard",
	"keywords": [
		"donation",
		"mollie",
		"wordpress"
	],
	"homepage": "https://kudosdonations.com",
	"repository": "https://github.com/mikey242/kudos-donations",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"bugs": {
		"url": "https://github.com/mikey242/kudos-donations/issues",
		"email": "michael@iseard.media"
	},
	"dependencies": {
		"@automattic/babel-plugin-preserve-i18n": "^1.0.0",
		"@headlessui/react": "2.2.7",
		"@heroicons/react": "^2.2.0",
		"@wordpress/dataviews": "^7.0.0",
		"clsx": "^2.1.1",
		"focus-trap-react": "^11.0.4",
		"nuqs": "^2.4.3",
		"react-hook-form": "^7.59.0",
		"react-mailchimp-subscribe": "^2.1.3",
		"react-router-dom": "^7.6.3",
		"react-select-country-list": "^2.2.3",
		"react-shadow": "^20.6.0",
		"zod": "^4.0.0"
	},
	"devDependencies": {
		"@babel/preset-react": "^7.27.1",
		"@tailwindcss/forms": "^0.5.10",
		"@types/codemirror": "^5.60.15",
		"@types/lodash": "^4.17.16",
		"@types/react-select-country-list": "^2.2.3",
		"@types/wordpress__blocks": "^12.5.17",
		"@wordpress/api-fetch": "^7.29.0",
		"@wordpress/babel-preset-default": "^8.29.0",
		"@wordpress/block-editor": "^15.2.0",
		"@wordpress/blocks": "^15.2.0",
		"@wordpress/browserslist-config": "^6.29.0",
		"@wordpress/components": "^30.2.0",
		"@wordpress/compose": "^7.29.0",
		"@wordpress/core-data": "^7.29.0",
		"@wordpress/data": "^10.29.0",
		"@wordpress/date": "^5.29.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^6.29.0",
		"@wordpress/dom-ready": "^4.29.0",
		"@wordpress/element": "^6.29.0",
		"@wordpress/env": "^10.29.0",
		"@wordpress/eslint-plugin": "^22.15.0",
		"@wordpress/hooks": "^4.29.0",
		"@wordpress/i18n": "^6.2.0",
		"@wordpress/keycodes": "^4.29.0",
		"@wordpress/notices": "^5.29.0",
		"@wordpress/scripts": "^30.22.0",
		"@wordpress/stylelint-config": "^23.21.0",
		"autoprefixer": "^10.4.21",
		"lodash": "^4.17.21",
		"postcss": "^8.5.6",
		"postcss-import": "^16.1.1",
		"prettier": "^3.6.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"stylelint": "^16.21.0",
		"stylelint-config-tailwindcss": "^1.0.0",
		"tailwindcss": "^3.4.17",
		"terser-webpack-plugin": "^5.3.14",
		"webpack": "^5.99.9",
		"webpack-cli": "^6.0.1",
		"webpack-merge": "^6.0.1"
	},
	"scripts": {
		"build": "npm run build:admin && npm run build:front",
		"build:admin": "wp-scripts build --config src/admin/webpack.config.admin.js --webpack-src-dir=src/admin --output-path=build/admin",
		"build:front": "wp-scripts build --config src/block/webpack.config.front.js --output-path=build/front",
		"check-engines": "wp-scripts check-engines",
		"start:admin": "wp-scripts start --hot --config src/admin/webpack.config.admin.js --webpack-src-dir=src/admin --output-path=build/admin",
		"start:front": "wp-scripts start --hot --config src/block/webpack.config.front.js --output-path=build/front",
		"wp-env": "wp-env",
		"wp-env:debug": "wp-env start --xdebug",
		"make-pot": "wp i18n make-pot . --exclude='src, wordpress, config, assets, tests' --debug",
		"make-json": "wp i18n make-json languages/ --no-purge",
		"start:tunnel": "cloudflared tunnel --config .cloudflared/config.yml run",
		"log:follow": "wp-env run cli tail -f wp-content/debug.log",
		"log:clear": "wp-env run cli truncate -s 0 wp-content/debug.log",
		"test:unit": "wp-env run tests-cli --env-cwd=wp-content/plugins/kudos-donations ./vendor/bin/phpunit",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style 'src/**/*.css'",
		"lint:js": "wp-scripts lint-js ./src",
		"packages-update": "wp-scripts packages-update",
		"make-zip": "rm -f kudos-donations.zip && php wp-cli.phar dist-archive . ./kudos-donations.zip --allow-root"
	},
	"wp-env": {
		"plugin-dir": "kudos-donations",
		"plugin-name": "Kudos Donations",
		"welcome-build-command": "npm run production"
	}
}
