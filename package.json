{
  "name": "kudos-donations",
  "version": "4.0.0-beta",
  "description": " Add a donation button to any page on your website. Easy & fast setup. Works with Mollie payments.",
  "homepage": "https://gitlab.iseard.media/michael/kudos-donations",
  "scripts": {
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production",
    "make-pot": "wp i18n make-pot . --exclude='docker, src'",
    "make-json": "wp i18n make-json languages/ --no-purge",
    "export": "composer install --no-dev --optimize-autoloader && ./export.sh"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "devDependencies": {
    "@automattic/babel-plugin-preserve-i18n": "^1.0.0",
    "@automattic/i18n-check-webpack-plugin": "^1.0.12",
    "@automattic/i18n-loader-webpack-plugin": "^2.0.8",
    "@babel/preset-react": "^7.17.12",
    "@tailwindcss/forms": "^0.5.2",
    "@wordpress/babel-plugin-makepot": "^4.3.1",
    "@wordpress/babel-preset-default": "^6.11.0",
    "@wordpress/block-editor": "^9.1.0",
    "@wordpress/blocks": "^11.8.0",
    "@wordpress/browserslist-config": "^4.1.0",
    "@wordpress/components": "^19.11.0",
    "@wordpress/compose": "^5.7.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.5.0",
    "@wordpress/eslint-plugin": "^12.3.0",
    "@wordpress/keycodes": "^3.9.0",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.3.0",
    "laravel-mix": "^6.0.38",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.18"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@headlessui/react": "1.6.1",
    "@heroicons/react": "^1.0.6",
    "@wordpress/api-fetch": "^6.6.0",
    "@wordpress/element": "^4.7.0",
    "@wordpress/hooks": "^3.2.1",
    "@wordpress/i18n": "^4.3.0",
    "autoprefixer": "^10.4.7",
    "classnames": "^2.3.1",
    "focus-trap-react": "^9.0.2",
    "lodash": "^4.17.21",
    "react": "^17.0.0",
    "react-colorful": "^5.5.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.26.0",
    "react-select-country-list": "^2.2.3",
    "react-shadow-root": "^6.1.1",
    "remove": "^0.1.5"
  }
}
